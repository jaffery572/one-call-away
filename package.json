{
  "name": "uk-callcenter-website",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise Call Center Website",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.{js,html,css}\"",
    "optimize-images": "node scripts/optimize-images.js",
    "deploy": "npm run build && netlify deploy --prod",
    "security-scan": "node scripts/security-scan.js",
    "performance-audit": "lighthouse https://ukcallcenter.netlify.app --output json --output-path ./reports/lighthouse.json"
  },
  "dependencies": {
    "lazysizes": "^5.3.2",
    "swiper": "^8.4.5",
    "vanilla-lazyload": "^17.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@parcel/transformer-sass": "^2.8.2",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "parcel": "^2.8.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.1",
    "sharp": "^0.31.3",
    "terser": "^5.16.1",
    "web-vitals": "^3.1.1"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 11"
  ]
}
